{
  "name": "AgriTrack",
  "short_name": "AgriTrack",
  "id": "AgriTrack",
  "start_url": "https://agri-clear.vercel.app/",
  "scope": "/",
  "description": "Tracking agricultural produce from farm to table.",
  "lang": "en-US",
  "dir": "ltr",
  "display": "standalone",
  "display_override": [
    "standalone",
    "tabbed",
    "window-controls-overlay",
    "fullscreen",
    "minimal-ui"
  ],
  "orientation": "portrait",
  "background_color": "#F5F5DC",
  "theme_color": "#F5F5DC",
  "categories": [
    "food",
    "business"
  ],
  "icons": [
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "/icons/icon-192x192.svg",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    },
    {
      "src": "/icons/icon-512x512.svg",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    }
  ],
  "shortcuts": [
    {
      "name": "New Produce",
      "short_name": "New",
      "description": "Add new produce",
      "url": "/add",
      "icons": [
        {
          "src": "/icons/shortcut-add.svg",
          "sizes": "192x192",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "Pending",
      "short_name": "Pending",
      "description": "View pending requests",
      "url": "/pending",
      "icons": [
        {
          "src": "/icons/shortcut-pending.svg",
          "sizes": "192x192",
          "type": "image/svg+xml"
        }
      ]
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/shot-1.svg",
      "sizes": "640x480",
      "type": "image/svg+xml"
    },
    {
      "src": "/screenshots/shot-2.svg",
      "sizes": "640x480",
      "type": "image/svg+xml"
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "id": "com.example.agritrack",
      "url": "https://play.google.com/store/apps/details?id=com.example.agritrack"
    }
  ],
  "prefer_related_applications": false,
  "iarc_rating_id": "PENDING",
  "scope_extensions": [
    "https://subdomain.agri-clear.vercel.app/"
  ],
  "file_handlers": [
    {
      "action": "/open",
      "accept": {
        "application/json": [
          ".json"
        ]
      }
    }
  ],
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "file",
          "accept": [
            "image/*"
          ]
        }
      ]
    }
  },
  "edge_side_panel": {
    "default": "/"
  }
}
